[
    "(:action cluster ; Cluster a resource and update its status\n  :parameters (?npc - agent ?resource - entity ?clustered_entity - entity) \n  :precondition (and \n                  (not (clustered ?clustered_entity)) \n                  (found ?resource)\n                )\n  :effect (clustered ?clustered_entity)\n)",
    "(:action get ;  Get a resource and update the collected status\n    :parameters (?npc - npc ?resource - object)\n    :precondition (not (collected ?resource))\n    :effect (collected ?resource)\n)",
    "(:action start_fire\n    :parameters (?n - npc ?c - camp ?ts - teepee_structure)\n    :precondition (and\n                    (clustered grass_tinder)\n                    (not (fire_started ?c))\n                  )\n    :effect (fire_started ?c)\n)",
    "(:action break_branch\n    :parameters (?npc - person ?tree - tree ?branch - branch)\n    :precondition (and\n                    (strong ?npc)\n                    (attached ?branch ?tree)\n                   )\n    :effect (and\n              (not (attached ?branch ?tree))",
    "(:action start\n    :parameters (?actor - entity ?target - entity ?context - entity)\n    :precondition (and\n                    (not (state_changed ?context))\n                    (appropriate_conditions ?actor ?target ?context)\n                  )\n    :effect (state_changed ?context)\n)",
    "(:action break_branch\n  :parameters (?n - npc ?b - bush ?bp - branch_piece)\n  :precondition (and\n                  (not (broken ?bp))\n                  (intact ?b)\n                )\n  :effect (and\n            (broken ?bp)\n            (not (intact ?b))",
    "(:action build_teepee\n  :parameters (?n - npc ?bp - branch_piece ?t - teepee)\n  :precondition (and\n                  (collected ?bp)\n                  (not (built ?t))\n                )\n  :effect (and\n            (built ?t)\n            (not (collected ?bp))",
    "(:action break\n  :parameters (?actor - entity ?target - object ?result - object)\n  :precondition (and\n                  (not (broken ?result))\n                  (intact ?target)\n                )\n  :effect (and\n            (broken ?result)\n            (not (intact ?target))",
    "(:action build\n    :parameters (?npc - person ?camp - camp ?item - object ?structure - structure_type)\n    :precondition (and \n                    (or (broken_branch ?item) (collected ?item) (fire_started ?camp))\n                    (not (built ?structure))\n                  )\n    :effect (and \n              (built ?structure)\n              (or (shelter_built ?camp) (cooking ?npc ?camp))",
    "(:action burn ; Burn a combustible item to prepare or transform it\n  :parameters (?actor - person ?item - combustible)\n  :precondition (not (burned ?item))\n  :effect (burned ?item))",
    "(:action collect ; Collect a resource and update its status\n  :parameters (?npc - npc ?resource - object)\n  :precondition (not (collected ?resource))\n  :effect (collected ?resource)\n)"
]